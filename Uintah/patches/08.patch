+
+template<VarType VAR, StnType STN>
+void Benchmark04<VAR, STN>::task_time_advance_v (
+    const ProcessorGroup * myworld,
+    const PatchSubset * patches,
+    const MaterialSubset *,
+    DataWarehouse * dw_old,
+    DataWarehouse * dw_new
+)
+{
