+void
+Benchmark03<VAR, STN>::scheduleComputeStableTimeStep (
+    const LevelP & level,
+    SchedulerP & sched
+)
+{
+    Task * task = scinew Task ( "Benchmark03::task_compute_stable_timestep ", this, &Benchmark03::task_compute_stable_timestep );
